; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="GdBoard"
run/main_scene="res://main_process/loader/loader.tscn"
config/auto_accept_quit=false
config/quit_on_go_back=false
config/features=PackedStringArray("4.3", "GL Compatibility")
run/low_processor_mode=true
boot_splash/bg_color=Color(0, 0, 0, 1)
boot_splash/show_image=false
boot_splash/fullsize=false
boot_splash/use_filter=false
config/icon="res://icon.png"

[autoload]

Settings="*res://autoload/settings.gd"
Api="*res://autoload/api.gd"
Projects="*res://autoload/projects.gd"
ApiNodes="*res://autoload/api_nodes.gd"
ApiMem="*res://autoload/api_memory.gd"

[debug]

gdscript/warnings/unassigned_variable=0

[display]

window/size/mode=2
window/subwindows/embed_subwindows=false
window/handheld/orientation=6

[editor_plugins]

enabled=PackedStringArray()

[file_customization]

folder_colors={
"res://Plugins/": "purple",
"res://autoload/": "green",
"res://core/": "yellow",
"res://main_process/": "red",
"res://theme/": "orange"
}

[gui]

theme/custom="res://theme/mctheme.tres"

